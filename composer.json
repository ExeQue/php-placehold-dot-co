{
    "name": "exeque/placehold-dot-co",
    "description": "Wrapper for Placehold.co placeholder images",
    "type": "library",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "pestphp/pest": "^3.7",
        "symfony/var-dumper": "^7.2",
        "laravel/pint": "^1.21",
        "mockery/mockery": "^1.6",
        "pestphp/pest-plugin-type-coverage": "^3.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ExeQue\\PlaceholdDotCo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Morten Harders",
            "email": "mmh@harders-it.dk"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "php vendor/bin/pest",
        "test:coverage": "php vendor/bin/pest --coverage",
        "test:types": "php vendor/bin/pest --type-coverage",
        "test:mutations": "php vendor/bin/pest --mutate",
        "test:all": [
            "@test",
            "@test:coverage",
            "@test:types",
            "@test:mutations"
        ]
    }
}
