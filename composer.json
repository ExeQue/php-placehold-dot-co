{
    "name": "exeque/placeholdco",
    "description": "Wrapper for Placehold.co placeholder images",
    "type": "library",
    "require": {
        "php": "^8.4",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "pestphp/pest": "^3.7",
        "symfony/var-dumper": "^7.2",
        "symfony/cache": "^7.2",
        "laravel/pint": "^1.21",
        "mockery/mockery": "^1.6",
        "pestphp/pest-plugin-type-coverage": "^3.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ExeQue\\PlaceholdDotCo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Morten Harders",
            "email": "mmh@harders-it.dk"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
